@model IEnumerable<OrdenAC.Models.OrdenModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@{
    ViewBag.Title = "Consulta de orden";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Listado de Órdenes</h2>

<p>
    @Html.ActionLink("Registrar", "RegistrarOrden")
</p>

<table class="table">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrdenModelId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OficinaVentaId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cliente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producto)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrdenModelId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRegistro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OficinaVentaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Producto)
            </td>
        </tr>

    }
</table>